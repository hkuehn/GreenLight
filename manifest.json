{
	"manifest_version": 2,

	"name": "GreenLight",

	"description": "Tells if image is NSFW",
	"version": "0.1",

	"browser_action": {
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},

	"options_page" : "options.html",

	"background": {
		"scripts" : ["eventPage.js"],
		"persistent" : false
	},

	"permissions" : [
		"storage",
		"notifications",
		"contextMenus"
	]
}